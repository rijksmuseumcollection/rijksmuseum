{{#if artpiece}}
  {{#each artpiece}}
    <div>
      {{#if image}}
      <a href="../../../artpiece/{{apiId}}"><img src="{{image}}" alt="{{title}}"></a>
      {{else}}
      <img src="../images/popino.jpg">
      {{/if}}
      <p>{{title}}</p>
      <p>{{author}}</p>
    </div>
  {{/each}}
{{else}}
  <p>There's no art in this collection :(</p>
{{/if}}

